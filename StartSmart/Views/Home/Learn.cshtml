@model HomeDetailsViewModel

@{
    ViewBag.Title = "StartSmart Profile";

}


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width , initial-scale=1.0" />
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.bundle.min.js"></script>
    <link href="~/css/bootstrap.min.css" rel="stylesheet">


    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js"></script>


    <!--<link href="Welcomenavbar.css" rel="stylesheet" />-->
    <!-- <link href="Welcome.css" rel="stylesheet" /> -->

    <link href="~/css/navbar.css" rel="stylesheet">
    <link href="~/css/Homepage.css" rel="stylesheet">

    <title>StartSmart: Learn Modules</title>
</head>
<body>
    <hr />
    <div class="container-fluid">
        <div class="section d-none d-lg-block">
            <div class="row">
                <div class="col-8 d-flex justify-content-start">
                    <a asp-action="WelcomePage" asp-controller="Home" asp-route-id="@Model.User.Id"><img class="logo" src="~/images/brand_img.png" alt="logo" /></a>
                    <a class="top_bar" asp-action="Learn" asp-controller="Home" asp-route-id="@Model.User.Id">Learn</a>
                    <a class="top_bar" asp-action="Events" asp-controller="Home" asp-route-id="@Model.User.Id">Events</a>
                    <a class="top_bar" href="#">Network</a>
                </div>
                <div class="col-4 d-flex justify-content-end">
                    <a class="cta" asp-action="Details" asp-controller="Home" asp-route-id="@Model.User.Id"><button>My Profile</button></a>
                    <a class="cta_no_margin" asp-action="Index" asp-controller="Home"><button>Log out</button></a>
                </div>
            </div>
        </div>
    </div>

</body>

<hr />
<hr />
<hr />
<hr />
<body>
    <div class="container-fluid">
        <div class="section d-none d-lg-block">
            <div class="row justify-content-start">

                <div class="offerings text-center col d-flex justify-content-left align-self-left">
                    <h1 class="heading_1">Learn</h1>
                </div>
            </div>
            <hr /><hr /><hr />
            <div class="row justify-content-start">

                <div class="offerings text-center col d-flex justify-content-left align-self-left">
                    <h1 class="heading_2">Your Learn Progress</h1>
                </div>
            </div>
            <hr /><hr />
            <div class="row justify-content-start">

                <div class="offerings text-center col d-flex justify-content-left align-self-left">
                    <h1 class="sub_1">Lesson 2</h1>
                </div>
            </div>
            <div class="row justify-content-start">

                <div class="offerings text-center col-2 d-flex align-self-center">
                    <h1 class="heading_3">Business Plan</h1>
                </div>
                <div class="progressBar">
                    <div class="progress" style="height:2.5em;">
                        <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuenow="75" aria-valuemin="0" aria-valuemax="100" style="width: 75%;height:2.5em;">75%</div>
                    </div>
                </div>
            </div>

            <hr /><hr /><hr />
            <div class="row justify-content-start">
                <div class="col">
                   <a asp-action="Videolearn" asp-controller="Home" asp-route-id="@Model.User.Id"><button class="cont_learning">Continue Module 2</button></a>

                </div>
            </div>

            <hr /><hr /> <hr /><hr /><hr /><hr />
            <div class="row justify-content-start align-self-center">
                <div class="offerings text-center col-10 d-flex">
                    <h1 class="heading_2">Modules</h1>
                </div>
            </div>

            <hr /><hr />
            <div class="row">

                <div class="col d-flex justify-content-center">
                    <img class="event_placeholder" src="~/images/Modules.png" alt="logo" />
                </div>
            </div>
        </div>
    </div>

    <hr /><hr />  <hr /><hr />  <hr /><hr />  <hr /><hr />  <hr /><hr />  <hr /><hr />  <hr /><hr />

    <footer id="custom_footer" class="py-4 text-white-50 d-none d-lg-block">
        <div class="container-fluid text-center">
            <div class="section ">
                <img class="logo" src="~/images/brand_img.png" alt="logo" />
                <small>Copyright &copy; StartSmart</small>
            </div>
        </div>
    </footer>

</body>
</html>